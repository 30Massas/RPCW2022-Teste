extends layout 

block content
  .w3-container.w3-margin
    h1.w3-center Termos de índice
    table.w3-table-all
      tr
        th.w3-center.w3-green Termo
        th.w3-center.w3-green ID Classe
        th.w3-center.w3-green Classe
      each d in data
        tr
          td.w3-center= d.termo
          td.w3-center
            a(href="/classes/"+d.idClasse)= d.idClasse
          td.w3-center
            a(href="/classes/"+d.idClasse)= d.tituloClasse
  .w3-container.w3-margin
    .w3-footer
        a.w3-left(href="/") Página Inicial
        a.w3-right(href=prev) Página Anterior